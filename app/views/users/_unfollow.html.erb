<!-- partial: unfollow -->
<!-- delete a new relationship -->
<!-- sends a DEL request to the Relationship model object to delete a relationship -->
<%= form_for(current_user.relationships.find_by_followed_id(@user),
             html: { method: :delete },
             remote: true) do |f| %>
  <%= f.submit "Unfollow", class: "btn btn-large" %>
<% end %>