<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <!-- shim for <IE9 browsers -->
    <%= render 'layouts/shim' %>
    
  </head>
  <body>
    <!-- Header -->
    <%= render 'layouts/header' %>
    
    <!-- Content -->
    <div class="container">
      <%= yield %>
      <!-- Footer -->
      <%= render 'layouts/footer' %>
    </div>


  </body>
</html>

